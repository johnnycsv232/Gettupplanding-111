    "clsx": "^2.1.1",
    "firebase": "^12.8.0",
    "framer-motion": "^12.27.5",
    "lucide-react": "^0.562.0",
    "next": "16.1.5",
    "react": "19.2.3",
    "react-dom": "19.2.3",
