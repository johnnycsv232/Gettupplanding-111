{
  "name": "gettupplanding",
  "version": "0.1.0",
  "private": true,
  "sideEffects": false,
  "scripts": {
    "format": "prettier --write .",
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "eslint",
    "test": "npm run test:unit",
    "test:unit": "vitest --config vitest.unit.config.ts --run",
    "test:policy": "vitest --config vitest.policy.config.ts --run",
    "test:e2e": "npm run test:e2e:dev",
    "test:e2e:dev": "tsx scripts/run-playwright.ts dev --project=chromium",
    "test:e2e:ci": "tsx scripts/run-playwright.ts ci --project=chromium",
    "full_audit": "npm run lint && npm run type-check && npm run test",
    "ui_check": "npm run test:e2e:dev",
    "skill_sync": "npm run skills:index && npm run skills:validate",
    "performance_audit": "tsx scripts/lighthouse_audit.ts",
    "security_scan": "tsx scripts/security_scan.ts",
    "api_validation": "tsx scripts/api_validator.ts",
    "schema_validation": "tsx scripts/schema_validator.ts",
    "integration_map_audit": "tsx scripts/integration_map_audit.ts",
    "bundle_analysis": "tsx scripts/bundle_analyzer.ts",
    "accessibility_audit": "tsx scripts/accessibility_audit.ts",
    "i18n_validation": "tsx scripts/i18n_validator.ts",
    "verify:landing": "npm run test:policy && npm run type-check && npm run test:unit && npm run test:e2e:dev && npm run build",
    "type-check": "tsc --noEmit",
    "verify": "tsx scripts/verify.ts",
    "setup": "tsx scripts/run-powershell.ts scripts/complete-setup.ps1",
    "cleanup": "tsx scripts/run-powershell.ts scripts/cleanup-ghost-code.ps1",
    "skills:list": "tsx scripts/skills-manager.ts list",
    "skills:disabled": "tsx scripts/skills-manager.ts disabled",
    "skills:enable": "tsx scripts/skills-manager.ts enable",
    "skills:enable-all": "tsx scripts/skills-manager.ts enable-all",
    "skills:disable": "tsx scripts/skills-manager.ts disable",
    "skills:index": "tsx scripts/generate-index.ts",
    "skills:validate": "tsx scripts/validate-skills.ts",
    "skills:validate:strict": "tsx scripts/validate-skills-report.ts",
    "skills:sync": "tsx scripts/sync-skills.ts",
    "skills:fix": "tsx scripts/fix-skills-validation.ts",
    "skillport": "tsx scripts/skillport.ts",
    "skillport:list": "tsx scripts/skillport.ts list",
    "skillport:show": "tsx scripts/skillport.ts show",
    "skillport:mcp": "tsx scripts/skillport.ts mcp",
    "skillport:reindex": "tsx scripts/skillport.ts reindex",
    "skillport:verify": "tsx scripts/run-powershell.ts scripts/verify-skillport.ps1",
    "rules:sync": "tsx scripts/sync-rules.ts",
    "analyze": "tsx scripts/build-analyze.ts",
    "doctor": "tsx scripts/doctor.ts --json",
    "prepare": "husky"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "prettier --write",
      "eslint --fix"
    ],
    "*.{json,css,md}": [
      "prettier --write"
    ]
  },
  "dependencies": {
    "@genkit-ai/dotprompt": "^1.0.0-rc.5",
    "@genkit-ai/firebase": "^1.28.0",
    "@genkit-ai/googleai": "^1.28.0",
    "@google/generative-ai": "^0.24.1",
    "@langchain/core": "^1.1.19",
    "@langchain/google-genai": "^2.1.15",
    "@langchain/langgraph": "^1.1.3",
    "@langchain/openai": "^1.2.5",
    "@next/bundle-analyzer": "^16.1.6",
    "@react-three/drei": "^10.7.7",
    "@react-three/fiber": "^9.5.0",
    "@react-three/postprocessing": "^3.0.4",
    "@sanity/client": "^7.14.0",
    "@stripe/stripe-js": "^8.6.3",
    "clsx": "^2.1.1",
    "firebase": "^12.8.0",
    "framer-motion": "^12.27.5",
    "genkit": "^1.28.0",
    "lenis": "^1.3.17",
    "lucide-react": "^0.562.0",
    "next": "^16.1.6",
    "react": "19.2.3",
    "react-dom": "19.2.3",
    "react-intersection-observer": "^10.0.2",
    "stripe": "^20.2.0",
    "tailwind-merge": "^3.4.0",
    "three": "^0.182.0"
  },
  "devDependencies": {
    "@firebase/rules-unit-testing": "^5.0.0",
    "@playwright/test": "^1.57.0",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.2",
    "@types/node": "^25.2.0",
    "@types/react": "^19.2.10",
    "@types/react-dom": "^19.2.3",
    "@types/three": "^0.182.0",
    "@vitejs/plugin-react": "^5.1.2",
    "autoprefixer": "^10.4.23",
    "eslint": "^9",
    "eslint-config-next": "16.1.4",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-tailwindcss": "^3.18.2",
    "eslint-plugin-unused-imports": "^4.3.0",
    "firebase-admin": "^12.7.0",
    "firebase-tools": "^15.3.1",
    "happy-dom": "^20.3.4",
    "husky": "^9.1.7",
    "jsdom": "^27.4.0",
    "lint-staged": "^16.2.7",
    "postcss": "^8.5.6",
    "prettier": "^3.8.1",
    "prettier-plugin-tailwindcss": "^0.7.2",
    "tailwindcss": "^3.4.17",
    "tsx": "^4.21.0",
    "typescript": "^5.9.3",
    "vitest": "^4.0.17",
    "zod": "^4.3.6"
  },
  "overrides": {
    "fast-xml-parser": "5.3.4"
  }
}
